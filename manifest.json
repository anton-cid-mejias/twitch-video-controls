{
    "manifest_version": 2,
    "name": "Twitch Video Control",
    "version": "1.0",
    "description": "Adds keyboard controls to the twitch html5 player.",
    "content_scripts": [
        {
            "matches": [
                "*://*.twitch.tv/*"
            ],
            "js": [
                "controls.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "256": "icons/256.png"
    },
    "applications": {
        "gecko": {
            "id": "twitch-video-control@thekonz.github.io"
        }
    }
}